<?xml version="1.0"?>
<configuration>
    <configSections>
        <section name="akka" type="Akka.Configuration.Hocon.AkkaConfigurationSection, Akka" />
    </configSections>

    <akka>
        <hocon>
            <![CDATA[
          akka {
            actor {
              deployment{
                /router1 {
                    router = consistent-hashing-pool
				            nr-of-instances = 3
                }
              }
            }          
          }
      ]]>
        </hocon>
    </akka>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
    </startup>
</configuration>
