{
    "metadata": [{
        "src": [{
            "files": ["**/*.csproj"],
            "exclude": [
                "**/obj/**",
                "**/bin/**",
                "_site/**",
                "**/examples/**",
                "**/benchmark/**",
                "**/*TestRunner*.csproj",
                "**/*Tests*.csproj",
                "**/*Tests.*.csproj"
            ],
            "src": "../src"
        }],
        "dest": "api",
        "properties": {
            "TargetFramework": "net45"
        }
    }],
    "build": {
        "content": [{
            "files": [
                "api/**.yml",
                "api/index.md"
            ]
        }, {
            "files": [
                "articles/**.md",
                "articles/**/toc.yml",
                "community/**.md",
                "community/**/toc.yml",
                "toc.yml",
                "*.md"
            ],
            "exclude": [
                "obj/**",
                "_site/**"
            ]
        }],
        "resource": [{
            "files": [
                "images/**",
                "web.config"
            ],
            "exclude": [
                "obj/**",
                "_site/**"
            ]
        }],
        "overwrite": [{
            "files": [
                "apidoc/**.md"
            ],
            "exclude": [
                "obj/**",
                "_site/**"
            ]
        }],
        "globalMetadata": {
            "_appTitle": "Akka.NET Documentation",
            "_appLogoPath": "/images/akkalogo.png",
            "_appFaviconPath": "/images/favicon.ico",
            "_appFooter": "Copyright ï¿½ 2013-2016 Akka.NET project<br>Generated by <strong>DocFX</strong>",
            "_enableSearch": "true"
        },
        "dest": "_site",
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "template": [
            "default",
            "template"
        ],
        "postProcessors": [],
        "noLangKeyword": false,
    }
}