{
    "version": "0.1",
    "language": "en",
    "words": [
        "Akka",
        "Allman",
        "appender",
        "backoff",
        "backpressure",
        "backpressures",
        "backpressuring",
        "Bootcamp",
        "combinator",
        "combinators",
        "composability",
        "compositionality",
        "CQRS",
        "CRDT",
        "denormalization",
        "Gitter",
        "HOCON",
        "journaled",
        "Lauck",
        "materializer",
        "MNTR",
        "Petabridge",
        "Quic",
        "Quickstart",
        "remoting",
        "roadmap",
        "runtimes",
        "Ryzen",
        "serializers",
        "Serilog",
        "substream",
        "substreams",
        "testkit",
        "Tweetinvi",
        "Varghese"
    ],
    "ignoreWords": [
        "Hanselminutes",
        "Resharper",
        "Bartosz",
        "Bartosz's"
    ],
    "ignoreRegExpList": [
        "\\((.*)\\)", // Markdown links
        "```[a-z]*\n[\\s\\S]*?\n```", // Markdown code blocks. h/t https://coderwall.com/p/r6b4xg/regex-to-match-github-s-markdown-code-blocks,
        "\\`([^\\`].*?)\\`", // inline code blocks. h/t https://stackoverflow.com/questions/41274241/how-to-capture-inline-markdown-code-but-not-a-markdown-code-fence-with-regex
        "\\<a(.*)\\>", // link contents
        "-- snippet:(.*)", // snippet references
        "\\<\\[sample:(.*)", // another kind of snippet reference
        "/^\\s*```[\\s\\S]*?^\\s*```/gm", // ignoring multi-line code blocks
        "/^---[\\s\\S]+?---/gm" //front-matter
    ]
}
